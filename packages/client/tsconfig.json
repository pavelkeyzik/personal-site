{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "src",
    "paths": {
      "hooks/*": ["hooks/*"],
      "components/*": ["components/*"],
      "utils/*": ["utils/*"],
      "routes": ["routes.ts"],
      "types": ["types.ts"]
    }
  }
}
